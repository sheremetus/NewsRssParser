            <version>1.3</version>
        </dependency>

        <dependency>
            <groupId>org.python</groupId>
            <artifactId>jython-slim</artifactId>
            <version>2.7.3b1</version>
        </dependency>



        <dependency>
            <groupId>com.google.code.gson</groupId>
